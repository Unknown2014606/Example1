<h1>list</h1>
<table>
  <tr>
    <th>title</th>
    <th>topic</th>
		<th>president</th>
  </tr>
 
  <% @videos.each do |video| %>
    <tr>
      <td><%= video.title %></td>
      <td><%= video.topic %></td>
			<td><%= video.president %></td>
			<td><%= link_to 'Show', admin_video_path(video)%></td>
			<td><%= link_to 'Edit', admin_video_edit_path(video)%></td>
			<td><%= link_to 'Destroy', admin_video_delete_path(video),
			                    method: :delete %></td>
    </tr>
  <% end %>
</table>
<td><%= link_to 'Back', admin_home_path%></td>